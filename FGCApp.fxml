<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
   <top>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="buttonSpreadsheetProperties" alignment="CENTER" minWidth="-Infinity" mnemonicParsing="false" onAction="#spreadsheetPropertiesClicked" prefHeight="26.0" prefWidth="140.0" text="Spreadsheet Properties">
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="buttonApplyFilter" alignment="CENTER" minWidth="-Infinity" mnemonicParsing="false" onAction="#applyFilterClicked" prefHeight="26.0" prefWidth="140.0" text="Apply Filter">
               <HBox.margin>
                  <Insets top="10.0" />
               </HBox.margin>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="buttonFilterHistory" alignment="CENTER" minWidth="-Infinity" mnemonicParsing="false" onAction="#filterHistoryClicked" prefHeight="26.0" prefWidth="140.0" text="Filter History">
               <HBox.margin>
                  <Insets right="10.0" top="10.0" />
               </HBox.margin>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
         </children>
      </HBox>
   </top>
</BorderPane>
